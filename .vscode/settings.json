{
  "cSpell.words": [
    "ENDPOINTENTRY",
    "middlewares",
    "oidc"
  ],
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.alwaysShowStatus": true,
  "typescript.format.enable": false,
  "tsimporter.filesToScan": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue"
  ],
  "tsimporter.filesToExclude": []
}